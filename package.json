{
  "name": "@gettersethya/svelte-image-gallery",
  "version": "1.1.7",
  "author": "Getter Sethya",
  "license": "MIT",
  "description": "Svelte image gallery ported from react-image-gallery",
  "homepage": "https://github.com/GetterSethya/svelte-image-gallery",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GetterSethya/svelte-image-gallery.git"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^3.0.9",
    "@sveltejs/kit": "~2.5.28",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@types/throttle-debounce": "~5.0.0",
    "@typescript-eslint/eslint-plugin": "~5.45.0",
    "@typescript-eslint/parser": "~5.45.0",
    "eslint": "~8.28.0",
    "eslint-config-prettier": "~8.5.0",
    "prettier": "~2.8.0",
    "prettier-plugin-svelte": "~2.10.1",
    "sass": "~1.57.1",
    "svelte": "~4.2.20",
    "svelte-check": "~3.6.7",
    "tslib": "~2.6.2",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  },
  "type": "module",
  "dependencies": {
    "@react2svelte/swipable": "^0.1.4",
    "@react2svelte/swipeable": "^0.1.4",
    "clsx": "^1.2.1",
    "throttle-debounce": "^5.0.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "svelte": "./dist/index.js",
  "module": "./dist/index.js",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "keywords": [
    "svelte",
    "sveltekit",
    "image",
    "gallery",
    "ssr"
  ],
  "files": [
    "dist"
  ]
}
